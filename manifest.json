{
  "manifest_version": 3,
  "name": "SSW.People.Profiles.CopyExtension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://ssw.com.au/people/*/*"],
      "js": ["src/content-script.js","src/libs/papaparse.min.js"],
      "css": ["src/assets/styles.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["src/assets/*"],
      "matches": ["*://*/*"]
    }
  ]
}
